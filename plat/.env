# JiSpeed 平台管理系统 - 基础环境配置

# 应用基础信息
VITE_BASE_URL=/
VITE_APP_TITLE=JiSpeed 平台管理系统
VITE_APP_DESC=基于 Vue3 + TypeScript + NaiveUI + UnoCSS 的外卖配送平台管理系统

# 图标配置
VITE_ICON_PREFIX=icon
VITE_ICON_LOCAL_PREFIX=icon-local

# 路由配置
VITE_AUTH_ROUTE_MODE=static
VITE_ROUTE_HOME=home
VITE_ROUTER_HISTORY_MODE=history

# 默认菜单图标
VITE_MENU_ICON=mdi:menu

# HTTP代理配置（开发环境）
VITE_HTTP_PROXY=Y

# 后端服务响应码配置
VITE_SERVICE_SUCCESS_CODE=200
VITE_SERVICE_LOGOUT_CODES=401
VITE_SERVICE_MODAL_LOGOUT_CODES=4001
VITE_SERVICE_EXPIRED_TOKEN_CODES=4002

# 超级管理员角色
VITE_STATIC_SUPER_ROLE=admin

# 开发工具配置
VITE_SOURCE_MAP=Y
VITE_STORAGE_PREFIX=jispeed-
VITE_AUTOMATICALLY_DETECT_UPDATE=Y
VITE_PROXY_LOG=Y
